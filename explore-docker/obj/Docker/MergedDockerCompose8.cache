services:
  webapi:
    build:
      context: C:\Tarun\poc\aks\explore-docker\explore-docker
      dockerfile: webApi/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: webapi
    ports:
    - published: 52080
      target: 80
    - published: 51443
      target: 443
    volumes:
    - C:\Users\v-tjain\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\v-tjain\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
  webapp:
    build:
      context: C:\Tarun\poc\aks\explore-docker\explore-docker
      dockerfile: WebApp/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
